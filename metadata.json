{
  "name": "simp-vsftpd",
  "version": "7.9.0",
  "author": "SIMP Team",
  "summary": "Manage vsftpd",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-vsftpd",
